package stringSearcher;

public class BoyerMooreTable {
    private char letter;
    private int value;

    public  BoyerMooreTable(char setLetter, int setValue){
        letter = setLetter;
        value = setValue;
    }

    /**
     * Gets the letter from the table.
     *
     * @return the tables letter.
     */
    public char getLetter() {
        return letter;
    }

    /**
     * Gets the value from the table.
     *
     * @return the tables value.
     */
    public int getValue() {
        return value;
    }
}
